    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "npm",
            "preLaunchTask": "npm: build",
            "runtimeArgs": ["run-script", "start"],
            "env": {
                "APP_ENV": "dev"
            },
            "console": "externalTerminal"
        }
    ]
}